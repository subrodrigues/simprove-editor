<StackPane fx:id="multiSelectListRoot" minHeight="100.0" prefWidth="600.0" styleClass="float-window-style" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">

    <children>
        <VBox>
            <children>
                <StackPane fx:id="headerPane" alignment="TOP_CENTER" prefHeight="100.0" prefWidth="150.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Text fx:id="title" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="edit-dialog-title" text="Title">
                            <StackPane.margin>
                                <Insets top="16.0" />
                            </StackPane.margin>
                        </Text>
                    </children>
                </StackPane>

                <TabPane fx:id="tabPane">
                    <tabs>
                        <Tab fx:id="firstTab" closable="false" text="Tab1">
                            <JFXListView fx:id="firstList">
                            </JFXListView>
                        </Tab>
                        <Tab fx:id="secondTab" closable="false" text="Tab2">
                            <JFXListView fx:id="secondList">
                            </JFXListView>
                        </Tab>
                    </tabs>

                    <opaqueInsets>
                  <Insets />
               </opaqueInsets>
                </TabPane>
            </children>
        </VBox>
    </children>
</StackPane>
