			"behavior": String,
			"effectTime": int,
			"usageLimit": int,
			"conditions": [int], // state IDs
			"results": [ // array signals
				{
					"id": int,
					"name": String,
					"value": String
				}
			],
			"transition": {
				"id": int,
				"stateId": int
			},
			"score": {
				"startStates": [int], // state IDs
				"endStates": [int], // state IDs
				"lostValue": float, // Immediately lost value
				"lossOvertime": float // Value that will be lost overtime (until completion...)
			}
		}
	]
}